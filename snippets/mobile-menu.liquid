            <button class="mobile-menu__nav-link" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="mobile-panel-11" data-action="open-panel">
              <div class="menu-icon"><img src="{{block.settings.m_mob_menu_icon | img_url: '' }}" alt=""></div> <span>{{block.settings.title}}</span> {%- render 'icon', icon: 'arrow-right' -%}
            </button>
          </li>
          {% endif %}
          {% endfor %}
          {% for block in section.blocks %}
          {% if block.type == 'workstations_menu' %}
          <li class="mobile-menu__nav-item lopa">
            <button class="mobile-menu__nav-link" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="mobile-panel-12" data-action="open-panel">
              <div class="menu-icon"><img src="{{block.settings.w_mob_menu_icon | img_url: '' }}" alt=""></div> <span>{{block.settings.title}}</span> {%- render 'icon', icon: 'arrow-right' -%}
            </button>
          </li>
          {% endif %}
          {% endfor %}
          {% for block in section.blocks %}
          {% if block.type == 'crs_menu' %}
          <li class="mobile-menu__nav-item lopa">
            <button class="mobile-menu__nav-link" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="mobile-panel-13" data-action="open-panel">
              <div class="menu-icon"><img src="{{block.settings.crs_mob_menu_icon | img_url: '' }}" alt=""></div> <span>{{block.settings.title}}</span> {%- render 'icon', icon: 'arrow-right' -%}
            </button>
          </li>
          {% endif %}
          {% endfor %}
          {% for block in section.blocks %}
          {% if block.type == 'film_menu' %}
